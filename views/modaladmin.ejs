
  <!--Sign in Modal starts here-->
  <div class="modal-parent-signin" id="signin">
    <div class="modal-signin">
        <h1 style="text-align: center;">Log in</h1>
        <form action="/signin" method="post" role="form">
          <label for="username">Username</label><br/>
          <input name="uname" type="username" placeholder="Enter Your Username..." required/><br/><br/>
          <label for="password">Password</label><br/>
          <input name="password" type="password" placeholder="Enter Your Password..." required/><br/><br/>                            
          <button type="submit">Sign in</button><button type="reset">Reset</button><button onclick="closeSigninModal()">cancel</button><br/><br/>
          <a href="/forgotusername">Forgot Username</a>
          <a href="/forgotpassword">Forgot Password</a>                
          <a href="#" onclick="openSignUpModal()">Register</a>
        </form>
        <span class="X-signin">&times;</span>
    </div>
  </div>
  <!--Sign in Modal ends here-->
  
  <!--Sign up Admin Modal starts here-->
  <div class="modal-parent-signup-admin">
    <div class="modal-signup-admin">
        <h1 style="text-align: center;">Admin Staff Registeration</h1>
        <h3 style="text-align: center;">Customers, please Register <a type="button" href="#" onclick="openSignUpModalCustomerFromModalAdmin()" style="text-decoration: none;">here</a></h3>        
        <form action="/admin/signupadmin" method="post" role="form" enctype="multipart/form-data">
          <label for="firstname">First Name</label><br/>
          <input name="firstname" type="text" placeholder="Enter Your First Name..." /><br/><br/>
          <label for="lastname">Last Name</label><br/>
          <input name="lastname" type="text" placeholder="Enter Your Last Name..." /><br/><br/>
          <label for="usrname">Username</label><br/>
          <input name="usrname" type="username" placeholder="Create a Username..."  /><br/><br/>
          <label for="mobilenumber">Mobile Number</label><br/>
          <input name="mobilenumber" type="tel" placeholder="Enter Your Mobile Number..." /><br/><br/>
          <label for="email">Email</label><br/>
          <input name="email" type="email" placeholder="Enter Your Email id..." /><br/><br/>
          <label for="nationalid">National Id</label><br/>
          <input name="nationalid" type="nationalid" placeholder="Enter Your National id..." /><br/><br/>
          <label for="nationalid">Upload National Id Image</label><br/>
          <input name="nationalidimage" type="file" /><br/><br/>         
          <button type="submit">Proceed to Sign up</button><button type="reset">Reset</button><button type="button" onclick="closeSignupAdminModal()">cancel</button><br/><br/>
          
          <a href="#" onclick="openSignInModal()">Log in</a>
        </form>
        <span class="X-signup-admin" onclick="closeSignupAdminModal()">&times;</span>
    </div>
  </div>
  <!--Sign up Admin Modal ends here-->

